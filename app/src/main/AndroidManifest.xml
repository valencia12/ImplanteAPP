<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.appicoclear">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.example.appicoclear.activites.viewsAccounts.actionTherapist.playSounds">
        </activity>
        <activity android:name="com.example.appicoclear.activites.viewsAccounts.DataPatient">
        </activity>
        <activity android:name="com.example.appicoclear.activites.viewsAccounts.RegisterPatient">
        </activity>
        <activity android:name="com.example.appicoclear.activites.viewsAccounts.HomeUser">
        </activity>
        <activity android:name="com.example.appicoclear.activites.loginAction.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>
    </application>

</manifest>